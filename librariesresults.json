 'markup': '{% load filters %}\n<section class=\'public-domain-abstract\'>\n    <p>{{content.abstract|safe}}</p>\n</section>\n<style>\n    .public-domain-set > li {\n        padding-bottom: 10px;\n    }\n</style>\n<section class="public-domain"><a id="reuse" name="reuse"></a>\n    <ul class=\'search-results grid-view public-domain-results public-domain-set\'>\n        {% for item in content.set.items %}\n            <li>\n                <figure>\n                    <a href="{{item.link}}">\n                        <img alt="{% if item.alt %}{{item.alt}}{% endif %}"\n                            data-src="{{item.image}}"\n                            src="{{item.image}}">\n                        <figcaption>{{item.title|safe}}</figcaption>\n                    </a>\n                </figure>\n            </li>\n        {% endfor %}\n    </ul>\n</section>\n\n<script>\n    $(document).ready(function() {\n        // public domain\n        var $grid = $(\'.public-domain .grid-view\').isotope({\n            {% if content.set.layout == "aligned" %}\n            // For items that needs to be ordered\n            layoutMode: \'fitRows\',\n            {% endif %}\n            itemSelector: \'.public-domain .grid-view > li\',\n            percentPosition: true\n        });\n\n        $grid.imagesLoaded().progress( function() {\n            $grid.isotope(\'layout\');\n        });\n\n        // UX Interactions - Programs, Top Searches, Trending, Your Library, Exhibitions, Events, News, Public Domain\n        if (window[\'loc_ux_tracking\'] != undefined){\n            $(\'.public-domain-results.search-results li figure a\').click(function() {\n                var listItem = $(this).closest(\'li\');\n                var index = $(listItem).parent().children().index(listItem) + 1;\n                var label = \'[\' + index + \']:\' + $(this).closest(\'.public-domain-results li\').contents().text().trim();\n                loc_ux_tracking.trackUserInteractionEvent($(this), \'Public Domain\', \'Click\', label);\n            });\n\n        }\n\n    });\n</script>\n',
